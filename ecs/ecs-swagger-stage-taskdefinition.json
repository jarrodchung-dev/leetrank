{
  "family": "leetrank-swagger-stage-td",
  "containerDefinitions": [
    {
      "name": "swagger",
      "image": "%s.dkr.ecr.us-east-1.amazonaws.com/leetrank-swagger:staging",
      "essential": true,
      "memoryReservation": 300,
      "portMappings": [
        {
          "hostPort": 0,
          "protocol": "tcp",
          "containerPort": 8080
        }
      ],
      "environment": [
        {
          "name": "URL",
          "value": "swagger.json"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "leetrank-swagger-stage",
          "awslogs-region": "us-east-1"
        }
      }
    }
  ]
}
